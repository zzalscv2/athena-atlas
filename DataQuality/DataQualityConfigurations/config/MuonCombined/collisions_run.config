#######################
# Histogram Assessments
#######################


dir MuonPhysics {
    algorithm = MCP_10_events
    
    dir Overview  {
        output = MuonTracking/Expert/vsLB

        dir Z    {
            hist Overview_Z_nZ_LB {
                output = MuonTracking/Shifter/Z
                algorithm = MCP_10_events
                description = Number of Z observed in a LumiBlock
            }
        } #dir Z
        
        dir Jpsi {
            hist Overview_Jpsi_nJpsi_LB {
                output = MuonTracking/Shifter/Jpsi
                algorithm = MCP_10_events
                description = Number of Jpsi observed in a LumiBlock
            }   
        } #dir Jpsi
        
        dir CBMuons {
            hist Overview_CBMuons_nMuon_LB {
                output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of Muons observed in a LumiBlock
            }
            hist Overview_CBMuons_Trig {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of combined/not combined Muons with/without trigger
            }
            hist Overview_muons_per_event {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of Muons per event
            }
            hist Overview_muons_per_trig_event {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of Muons in triggered events
            }
            hist Overview_CBmuons_per_notrig_event {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of combined Muons in not triggered events
            }
            hist NoTrig_Overview_CBMuons_nMuon_LB {
                description = Number of Muons observed in a LumiBlock
            }
        } #dir CBMuons

        dir NonCBMuons {
            hist Overview_NonCBMuons_nMuon_LB {
                algorithm = MCP_10_events
            }
            hist Overview_NonCBmuons_per_trig_event {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of non combined Muons in triggered events
            }
            hist Overview_NonCBmuons_per_notrig_event {
                #output = MuonTracking/Shifter/Muons
                algorithm = MCP_10_events
                description = Number of non combined Muons in not triggered events
            }
            hist NoTrig_Overview_NonCBMuons_nMuon_LB {
                description = Number of Muons observed in a LumiBlock
            }
        } #dir NonCBMuons
        
        dir Container {
            hist Overview_Container_nSegment_LB {
                output = MuonTracking/Shifter/Reco
                algorithm = MCP_10_events
                description = Number of Segments observed in a LumiBlock
            }    
            hist Overview_Container_nMuonTrack_LB {
                output = MuonTracking/Shifter/Reco
                algorithm = MCP_10_events
                description = Number of Tracks observed in a LumiBlock
            }    
        } #dir Container

    } #dir Overview

