#######################
# Lumi Blocks
#######################
dir lowStat_LB(?P<LB>.*) {
  regex = 1
  algorithm = GatherData
  dir L1Calo {
    dir PPM {
      dir LUT-CP {
        dir EtaPhiMaps {
          hist ppm_em_2d_etaPhi_tt_lutcp_Threshold06@gs${LB} {
            output = L1Calo/.GlobalSummary/LumiRanges/LutCP/EM
          }
          hist ppm_had_2d_etaPhi_tt_lutcp_Threshold06@gs${LB} {
            output = L1Calo/.GlobalSummary/LumiRanges/LutCP/HAD
          }
        }
      }
      dir LUT-JEP {
        dir EtaPhiMaps {
          hist ppm_em_2d_etaPhi_tt_lutjep_Threshold06@gs${LB} {
            output = L1Calo/.GlobalSummary/LumiRanges/LutJEP/EM
          }
          hist ppm_had_2d_etaPhi_tt_lutjep_Threshold06@gs${LB} {
            output = L1Calo/.GlobalSummary/LumiRanges/LutJEP/HAD
          }
        }
      }
    }
  }
}

######################
# Other Histograms
######################

dir HLT {
  algorithm = GatherData
  dir METMon {
    dir Expert {
      dir L1_vs_HLT {
        hist L1_HLT_EtCor@gs {
          output = L1Calo/.GlobalSummary/MET
          description = Original location HLT/TRMET/Expert/L1_vs_HLT. L1 missing et from LVL1EnergySumRoI container, HLT missing et from LVL1EnergySumRoI container
        }
        hist L1_HLT_dEx@gs {
          output = L1Calo/.GlobalSummary/MET
          description = Original location HLT/TRMET/Expert/L1_vs_HLT. L1 missing et x - HLT missing et x
        }
        hist L1_HLT_dEy@gs {
          output = L1Calo/.GlobalSummary/MET
          description = Original location HLT/TRMET/Expert/L1_vs_HLT. L1 missing et y - HLT missing et y
        }
      }
    }
    dir Shifter {
      dir L1 {
        hist Eff_L1_XE50@gs {
          output = L1Calo/.GlobalSummary/MET
          description = Original location HLT/TRMET/Shifter/L1. Number of events pass the L1_XE50 trigger/total number of events
        }
      }
    }
  }
  dir JetMon {
    dir L1 {
      hist L1Jet_n@gs {
        display = LogY
        algorithm = HLTjet_KolmogorovTest_MaxDist
        output = L1Calo/.GlobalSummary/Jets
        description = Original location HLT/TRJET/SHIFTER/L1. Red means: mean of the histogram different from reference. NJet plot: this plot should be close to the reference. Red can be due to: 1) Low statistics wrt reference. 2) Current bunch spacing different from reference. 3) Strange shape. i.e. bumps at high multiplicity.
      }
      hist L1Jet_Et@gs {
        display = LogY
        algorithm = HLTjet_KolmogorovTest_MaxDist
        output = L1Calo/.GlobalSummary/Jets
        description = Original location HLT/TRJET/SHIFTER/L1. Red means: mean of the histogram different from reference. Et plot: This plot should be close to the reference. Red can be due to: 1) The histogram threshold is not the same as HLT threshold. 2) Slope of the histogram significantly different from reference. 3) Strange shape, i.e. bumps at high Et.
      }
    }
    dir OF {
      dir AntiKt10LCTopoJets {
        dir TrigEff {
          hist L1_J100_pt_vs_OF_pt@gsKt10 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Correlation
          }
          hist L1_J100_Resolution_pt@gsKt10 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Resolution
          }
          hist L1_J100_EffPt@gsKt10 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt10LCTopoJets/Turn-on
          }
        }
      }
      dir AntiKt4EMTopoJets {
        dir TrigEff {
          hist L1_J100_pt_vs_OF_pt@gsKt4 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt4EMTopoJets/Correlation
          }
          hist L1_J100_Resolution_pt@gsKt4 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt4EMTopoJets/Resolution
          }
          hist L1_J100_EffPt@sKt4 {
            regex = 1
            algorithm = HLTjet_Histogram_Not_Empty_with_Ref&GatherData
            output = L1Calo/.GlobalSummary/Jets
            description = Original location HLT/TRJET/EXPERT/OF/AntiKt4EMTopoJets/Turn-on
          }
        }
      }
    }
  }
  dir Egamma {
    algorithm = HLT_Histogram_Not_Empty_with_Ref&GatherData
    dir Expert {
      dir L1_EM22VHI {
        dir AbsResolutions {
          dir L1Calo {
            hist res_etVsEta@gsEM22VHI {
              display = StatBox
              output = L1Calo/.GlobalSummary/EGamma
              description = Original location HLT/TREG/Expert/L1_EM22VHI/AbsResolutions/L1Calo. https://twiki.cern.ch/twiki/bin/view/Atlas/TrigEgammaDataQualityAndMonitoring
            }
          }
        }
        dir Distributions {
          dir L1Calo {
            hist emClusVsEmIsol@gsEM22VHI {
              display = StatBox
              output = L1Calo/.GlobalSummary/EGamma
              description = Original location HLT/TREG/Expert/L1_EM22VHI/Distributions/L1Calo. https://twiki.cern.ch/twiki/bin/view/Atlas/TrigEgammaDataQualityAndMonitoring
            }
          }
        }
        dir Efficiency {
          dir L1Calo {
            hist eff_et@gsEM22VHI {
              display = StatBox
              output = L1Calo/.GlobalSummary/EGamma
              description = Original location HLT/TREG/Expert/L1_EM22VHI/Efficiency/L1Calo
            }
          }
        }
      }
      dir L1_EM20VH {
        dir Distributions {
          dir L1Calo {
            hist emClusVsEmIsol@gsEM20VH {
              display = StatBox
              output = L1Calo/.GlobalSummary/EGamma
              description = Original location HLT/TREG/Expert/L1_EM20VH/Distributions/L1Calo. https://twiki.cern.ch/twiki/bin/view/Atlas/TrigEgammaDataQualityAndMonitoring
            }
          }
        }
        dir Efficiency {
          dir L1Calo {
            hist eff_et@gsEM20VH {
              display = StatBox
              output = L1Calo/.GlobalSummary/EGamma
              description = Original location HLT/TREG/Expert/L1_EM20VH/Efficiency/L1Calo. https://twiki.cern.ch/twiki/bin/view/Atlas/TrigEgammaDataQualityAndMonitoring
            }
          }
        }
      }
    }
  }
  dir TauMon {
    dir Shifter {
      dir lowest_singletau {
        dir TurnOnCurves {
          hist TProfRecoL1PtEfficiency_shifter@gs {
            algorithm = TAU_HistKolmogorovTest_MaxDist
            display = StatBox
            output = L1Calo/.GlobalSummary/TAU
            description = Original location HLT/TRTAU/Shifter/lowest_singletau/TurnOnCurves
          }
          hist TProfRecoL1EtaEfficiency_shifter@gs {
            algorithm = TAU_HistKolmogorovTest_MaxDist
            display = StatBox
            output = L1Calo/.GlobalSummary/TAU
            description = Original location HLT/TRTAU/Shifter/lowest_singletau/TurnOnCurves
          }
        }
      }
    }
  }
}

