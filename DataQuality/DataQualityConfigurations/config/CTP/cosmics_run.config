############################################################
# CTP
############################################################

#############
# Output
#############

output top_level {
  output CentralTrigger {
    output ErrorSummary {
    }
    output CTP {
    }
    output MuCTPI {
      output Cand {
        output Expert {
        }
      }
      output Mult {
        output Expert {
        }
      }
      output Timing {
        output Expert {
        }
      }
      output TOB {
        output Expert {
        }
      }
    }
    output MuCTPI-SL {
    }
    output CTP-MuCTPI {
    }
  }
}

#######################
# Histogram Assessments
#######################

